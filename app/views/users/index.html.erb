<%= render 'users/info', user: @user,books: @books %>

<% @users.each do |book| %>
 <div class="col-md-8 offset-md-1">
      <h2>Users</h2>
      <table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td><%= attachment_image_tag @user, :profile_image, :fill, 100, 100, fallback: "no_image.jpg"%></td>
        <td><%= @user.name %></td>
        <td>
          <%= link_to user_path(@user.id) do %>
            <p>show</p>
          <% end %>
        </td>
      </tr>
  </tbody>
       </table>
 </div>
<% end %>